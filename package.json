      "prettier --write"
    ]
  },
  "dependencies": {
    "astro": "^4.0.7"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.44.0",
    "eslint-plugin-astro": "^0.31.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.0"
  }
}
