{
  "name": "astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write .",
    "format:check": "prettier .",
    "lint": "eslint --ext .astro,.js,.jsx,.ts,.tsx .",
    "prepare": "husky install"
  },
